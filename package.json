    "@prisma/client": "^2.0.0-beta.5",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/http-errors": "^1.6.3",
    "@types/uuid": "^7.0.3",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "http-errors": "^1.7.3",
    "uuid": "^8.0.0"
  }
